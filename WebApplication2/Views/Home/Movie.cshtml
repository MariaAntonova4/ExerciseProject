@{
    ViewData["Title"] = "Mooovies";
}

@model List<Movie>

<br></br><table class="table"><thead>
        <tr><th>Id</th>
            <th>Title<th/>
                <th>Date</th>
                <th>Action</th></tr>
    </thead>
    <tbody>
        @{
            foreach (var movie in Model)
            {
                
            <tr>
            <td>@movie.Id</td>
            <td>@movie.Title</td>
            <td>@movie.ReleaseDate</td>
            <td><a asp-action="ViewMovies" asp-route-id="@movie.Id">Edit</a></td>
            <td><a asp-action="DeleteMovies"asp-route-id="@movie.Id">Delete</a></td>
            </tr>}
        }
    </tbody>
</table>